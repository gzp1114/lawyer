<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.lawyer.pojo.InvestorInformation" table="investor_information"  >
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="registration_mark" type="java.lang.String">
            <column name="registration_mark"/>
        </property>
        <property name="litigant" type="java.lang.String">
            <column name="litigant"/>
        </property>
        <property name="capital_contribution" type="java.lang.String">
            <column name="capital_contribution"/>
        </property>
        <property name="type" type="java.lang.String">
            <column name="type"/>
        </property>
    </class>
</hibernate-mapping>
