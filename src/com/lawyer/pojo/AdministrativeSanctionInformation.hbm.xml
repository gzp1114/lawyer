<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.lawyer.pojo.AdministrativeSanctionInformation" table="administrative_sanction_information"  >
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="registration_mark" type="java.lang.String">
            <column name="registration_mark"/>
        </property>
        <property name="litigant" type="java.lang.String">
            <column name="litigant"/>
        </property>
        <property name="paperID" type="java.lang.String">
            <column name="paperID"/>
        </property>
        <property name="basic_facts" type="java.lang.String">
            <column name="basic_facts"/>
        </property>
        <property name="asserting_basis" type="java.lang.String">
            <column name="asserting_basis"/>
        </property>
        <property name="Punishment_basis" type="java.lang.String">
            <column name="Punishment_basis"/>
        </property>
        <property name="disciplinary_action" type="java.lang.String">
            <column name="disciplinary_action"/>
        </property>
        <property name="case_handling_organ" type="java.lang.String">
            <column name="case_handling_organ"/>
        </property>
        <property name="publication_date" type="java.lang.String">
            <column name="publication_date"/>
        </property>
        
    </class>
</hibernate-mapping>
